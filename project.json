{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.Mvc.Core": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    }
  },
  "runtimes": {
    "win10-x64": { },
    "osx.10.11-x64": { },
    "debian.8-x64": { }
  },
  "runtimeOptions": {
    "gcServer": true
  },

  "packOptions": {
    "licenseUrl": "",
    "projectUrl": "",
    "tags": [ "" ]
  },

  "excludeBuiltIn": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "config.json",
      "config.Production.json",
      "web.config"
    ]
  }
}
